{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "C_Cpp.updateChannel": "Insiders",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "tabnine.experimentalAutoImports": true,
  "git.autofetch": true,
  "code-runner.runInTerminal": true,
  "git.enableSmartCommit": true,
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true,
    "html": true,
    "css": true,
    "javascript": true,
    "python": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.requireConfig": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.startupEditor": "none",
  "bracketPairColorizer.depreciation-notice": false,
  "css.lint.unknownAtRules": "ignore",
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": {
  
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "Fira Code Regular, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "gitlens.defaultDateFormat": null,
  "gitlens.defaultDateShortFormat": null,
  "gitlens.defaultTimeFormat": null,
  "gitlens.defaultDateLocale": null,
  "gitlens.hovers.currentLine.over": "line",
  "scm.alwaysShowActions": true,
  "gitHistory.sideBySide": true,
  "gitHistory.alwaysPromptRepositoryPicker": true,
  "gitHistory.hideCommitViewExplorer": true,
  "githubPullRequests.pullBranch": "never",
  "yaml.schemas": {
    "file:///Users/ghulamyazdani/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.bitbucket.enabled": true,
  "editor.accessibilitySupport": "off",
  "liveServer.settings.donotShowInfoMsg": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "terminal.integrated.fontFamily": "FiraCode Nerd Font , monospace",
  "atlascode.jira.lastCreateSiteAndProject": {
    "siteId": "",
    "projectKey": ""
  },
  "workbench.iconTheme": "vscode-icons",
  "diffEditor.ignoreTrimWhitespace": false
}
